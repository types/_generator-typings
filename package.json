{
  "name": "create-typings",
  "prettyname": "Create Typings",
  "version": "2.0.0-alpha",
  "description": "Tool for creating TypeScript typings",
  "main": "dist/bin.js",
  "bin": {
    "createtypings": "dist/bin.js"
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "rimraf out && tsc && ava",
    "coverage": "rimraf out && tsc && nyc ava --check-coverage",
    "link+watch": "npm link && npm run watch",
    "watch": "rimraf dist && node scripts/watch.js",
    "verify": "npm run test && npm run lint && npm run build && npm run dependency-check",
    "dependency-check": "dependency-check . --unused --no-dev && dependency-check . --missing --no-dev",
    "preversion": "npm run verify",
    "version": "git add dist",
    "prepare": "typings install",
    "prepublishOnly": "npm run build",
    "postpublish": "git push"
  },
  "ava": {
    "files": [
      "dist/**/*.spec.js"
    ],
    "source": [
      "scripts/*.js",
      "dist/**/*.js",
      "*.json"
    ]
  },
  "license": "MIT",
  "author": {
    "name": "unional",
    "url": "https://github.com/unional"
  },
  "homepage": "https://github.com/typings/typingsrepo",
  "repository": {
    "type": "git",
    "url": "https://github.com/typings/typingsrepo.git"
  },
  "keywords": [
    "yeoman-generator",
    "typings",
    "tsd",
    "Definitely Typed",
    "d.ts"
  ],
  "engines": {
    "node": ">=4.0.0"
  },
  "dependencies": {
    "aurelia-logging": "^1.3.1",
    "aurelia-logging-color": "^0.5.5",
    "chalk": "^2.0.1",
    "change-case": "^3.0.1",
    "clibuilder": "^0.15.5",
    "es6-promise": "^4.1.1",
    "events": "^1.1.1",
    "extend": "^3.0.1",
    "inquirer": "^3.2.0",
    "mem-fs": "^1.1.3",
    "mem-fs-editor": "^3.0.2",
    "nyc": "^11.0.3",
    "rc": "^1.2.1",
    "simple-git": "^1.74.1",
    "typings-core": "^2.3.3",
    "update-notifier": "^2.2.0",
    "wordwrap": "^1.0.0",
    "xtend": "^4.0.1"
  },
  "devDependencies": {
    "@types/node": "^8.0.15",
    "ava": "^0.21.0",
    "ava-fixture": "^0.10.1",
    "dependency-check": "^2.9.1",
    "rimraf": "^2.6.1",
    "tslint": "^5.5.0",
    "tslint-config-unional": "^0.8.0",
    "typescript": "^2.4.2",
    "typings": "^2.1.1"
  }
}
