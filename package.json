{
  "name": "typingsrepo",
  "prettyname": "TypingsRepo",
  "version": "2.0.0-alpha",
  "description": "Generate typings repository",
  "homepage": "https://github.com/typings/typingsrepo",
  "author": {
    "name": "unional",
    "url": "https://github.com/unional"
  },
  "main": "dist/cli/bin.js",
  "bin": {
    "typingsrepo": "dist/cli/bin.js"
  },
  "files": [
    "dist/"
  ],
  "keywords": [
    "yeoman-generator",
    "typings",
    "tsd",
    "Definitely Typed",
    "d.ts"
  ],
  "engines": {
    "node": ">=4.0.0"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "change-case": "^3.0.0",
    "clibuilder": "^0.6.2",
    "commander": "^2.9.0",
    "es6-promise": "^3.2.1",
    "events": "^1.1.1",
    "extend": "^3.0.0",
    "inquirer": "^1.1.2",
    "mem-fs": "^1.1.3",
    "mem-fs-editor": "^2.3.0",
    "minimist": "^1.2.0",
    "nyc": "^10.0.0",
    "rc": "^1.1.6",
    "redux": "^3.5.2",
    "simple-git": "^1.43.0",
    "typings-core": "^2.0.0",
    "update-notifier": "^1.0.2",
    "wordwrap": "^1.0.0",
    "xtend": "^4.0.1",
    "yosay": "^1.2.0"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "ava-fixture": "^0.3.1",
    "dependency-check": "^2.6.0",
    "rimraf": "^2.5.4",
    "tslint": "^4.0.2",
    "tslint-config-unional": "^0.6.0",
    "typescript": "^2.0.0",
    "typings": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/typings/typingsrepo.git"
  },
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "rimraf out && tsc && ava",
    "coverage": "rimraf out && tsc && nyc ava --check-coverage",
    "link+watch": "npm link && npm run watch",
    "watch": "rimraf dist && node scripts/watch.js",
    "verify": "npm run test && npm run lint && npm run build && npm run dependency-check",
    "dependency-check": "dependency-check . --unused --no-dev && dependency-check . --missing --no-dev",
    "preversion": "npm run verify",
    "version": "git add dist",
    "prepare": "typings install",
    "prepublishOnly": "npm run build",
    "postpublish": "git push"
  },
  "ava": {
    "files": [
      "out/**/*.spec.js"
    ]
  },
  "license": "MIT"
}
